  <script>
function func(){
n=document.getElementById("a_znach").value;
y=n;
x=n%10;
a=(n-x)/10;
n=100*x+a;
document.getElementById("a").innerHTML ="<p>"+"<span>2.25=</span> <span>"+ n+"</span></p>";
x=(y-y%100)/100 ;
a=y%100 ;
n=10*a+x ;
document.getElementById("b").innerHTML ="<p>"+"<span>2.27=</span> <span>"+ n+"</span><p>";
x=((y-y%100)/10+y%10) 
n=10*x+(y%100-y%10)/10 
document.getElementById("c").innerHTML ="<p>"+"<span>2.29=</span> <span> "+ n+"</span></p>";
x=((y-y%100)/10+y%10) 
n=(y%100-y%10)*10+x 
document.getElementById("d").innerHTML ="<p>"+"<span>2.31=</span> <span> "+ n+"</span></p>";
x=y%10 
a=(y%100-x)+(y-y%100)/100 
n=100*x+a 
document.getElementById("e").innerHTML ="<p>"+"<span>2.33=</span> <span> "+ n+"</span></p>";
}
<script>
  <head>

